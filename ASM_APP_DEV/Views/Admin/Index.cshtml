@{
}

@model IEnumerable<User>

<h1>Hello admin</h1>

<div class="form-group">

                            <a class="nav-link text-dark"  asp-action="Categories">Manage Categories</a>
</div>

<table class="table">
  <thead>
    <tr>
      <th scope="col">Email</th>
      <th scope="col">Full Name</th>
      <th scope="col">Address</th>
      <th scope="col">Action</th>
    </tr>
  </thead>
  <tbody>
       @foreach(var item in Model){

    <tr> 
      <td>@item.Email</td>
      <td>@item.FullName</td>
      <td>@item.Address</td>
      <td href="">Reset Password</td>

    </tr> 
        }
  </tbody>
</table>
